  <joint name="rod_to_zrod_4" type="fixed">
    <parent link="rod_4"/>
    <child link="zrod_4"/>
    <origin xyz="0 0 0.125" rpy="-0.4 0.4 0"/>
  </joint>

  <joint name="zrod_to_prop_1" type="continuous">
    <parent link="zrod_1"/>
    <child link="prop_1"/>
    <origin xyz="0 0 0.05"/>
    <axis xyz="0 0 1"/>
  </joint>

  <joint name="zrod_to_prop_2" type="continuous">
    <parent link="zrod_2"/>
    <child link="prop_2"/>
