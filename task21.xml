  <joint name="zrod_to_prop_4" type="continuous">
    <parent link="zrod_4"/>
    <child link="prop_4"/>
    <origin xyz="0 0 0.05"/>
    <axis xyz="0 0 1"/>
  </joint>
  
</robot>
